_status = \"new\";\n  $input.all()[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n}\n\n// Return items\nreturn $input.all();\n"},"id":"b58550a8-6713-473b-82a0-f7c4fdb6ba1c","name":"isDiffOrNew","type":"n8n-nodes-base.code","position":[340,680],"typeVersion":1},{"parameters":{"dataType":"string","value1":"={{$json.github_status}}","rules":{"rules":[{"value2":"same"},{"value2":"different","output":1},{"value2":"new","output":2}]}},"id":"e006d927-5c5d-4647-8bc9-933504ab32c5","name":"Check Status","type":"n8n-nodes-base.switch","position":[740,680],"typeVersion":1},{"parameters":{},"id":"ece11a78-7344-4b95-a953-bcc3f8149433","name":"Same file - Do nothing","type":"n8n-nodes-base.noOp","position":[960,520],"typeVersion":1},{"parameters":{},"id":"2ec97615-5e84-4ffa-be07-46f76b573967","name":"File is different","type":"n8n-nodes-base.noOp","position":[960,680],"typeVersion":1},{"parameters":{},"id":"8d0c0966-d91c-4f13-8779-06b0aca056c4","name":"File is new","type":"n8n-nodes-base.noOp","position":[960,880],"typeVersion":1},{"parameters":{"resource":"file","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"repository":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_name }}"},"filePath":"={{ $('Config').first().item.repo_path }}{{ $json.subPath }}{{$('Execute Workflow Trigger').first().json.id}}.json","fileContent":"={{$('isDiffOrNew').item.json[\"n8n_data_stringy\"]}}","commitMessage":"={{$('Execute Workflow Trigger').first().json.name}} ({{$json.github_status}})"},"id":"e48364ce-87b4-40ab-8147-cf29371d4cb3","name":"Create new file","type":"n8n-nodes-base.github","position":[1180,880],"typeVersion":1,"credentials":{"githubApi":{"id":"d8Aca1Kz6HhErvT5","name":"GitHub account"}}},{"parameters":{"resource":"file","operation":"edit","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"repository":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_name }}"},"